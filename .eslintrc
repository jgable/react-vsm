{
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "__PRODUCTION__": false,
    "__DEMO_MODE__": false,
  },
  "parser": "babel-eslint",
  "plugins": [
    "react",
  ],
  "rules": {
    "curly": 2,
    "eol-last": 2,
    "eqeqeq": 2,
    "new-cap": 1,
    "no-bitwise": 2,
    "no-cond-assign": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-empty": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 1,
    "no-invalid-regexp": 2,
    "no-negated-in-lhs": 2,
    "no-octal": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unused-vars": [2, { "args": "none" }],
    "react/jsx-no-undef": 2,
    "react/jsx-key": 1,
    "semi": [2, "always"],
    "use-isnan": 2,
    "valid-typeof": 2,
  }
}
